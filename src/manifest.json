{
	"name": "ClojureScriptConsole",
	"version": "0.0.1",
	"description": "A ClojureScript console within the Web Inspector.",
	"devtools_page": "cljs.html",
    "icons": { "128": "128.png" },
	"minimum_chrome_version" : "18",
	"permissions": [
		"tabs",
		"contextMenus" 
	],
	"manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
